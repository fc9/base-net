{
    "name": "fc9/net",
    "description": "Making it easier to add support the multilevel network.",
    "keywords": [
        "fc9",
        "network",
        "multilevel",
        "multin√≠vel"
    ],
    "homepage": "https://github.com/fc9/base-network",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Fabio Cabral",
            "email": "fabiocabralsantos@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.2",
        "illuminate/support": "~7.1",
        "menarasolutions/geographer-laravel": "^0.2.1",
        "waavi/sanitizer": "^1.0"
    },
    "require-dev": {
        "orchestra/testbench": "^4.0",
        "phpunit/phpunit": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "Fc9\\Network\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Fc9\\Network\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Fc9\\Network\\Providers\\NetServiceProvider"
            ],
            "aliases": {
                "Queue": "Fc9\\Network\\Facades\\Network"
            }
        }
    }
}
